

stack Register:
    --> Virtual Register
    frameobj: offset(s0)
              
              addi t0, s0, offset
              lw a0, 0(t0)

    local array:
        loadaddressLocal %1, frameobj
        addi %1, s0, offset (base)
        
        lw a0, %1
        
LARegister:
    --> Virtual Register
    Gloabl var:
        spill -> nop 
        reload -> loadaddressGloabl
        
        lui %1, %hi(lable)

        addi %1, %lo(lable)
        lw %1, x(%1)         ->  lw %1, %lo(lable)(%1)
          
    tempFloatvar:
        spill -> nop 
        reload -> loadaddressGloabl

spill: 
    更改逻辑